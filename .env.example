# Copy this file and rename it to .env, then replace the values with your actual sensitive data

# OpenAI API Key
OPENAI_API_KEY=your-openai-api-key

# Flask Secret Key
FLASK_SECRET_KEY=your-flask-secret-key

# Database URL
DATABASE_URL=your-database-url

# Email Server
MAIL_SERVER=your-mail-server
MAIL_PORT=your-mail-port
MAIL_USE_TLS=your-mail-use-tls
MAIL_USERNAME=your-mail-username
MAIL_PASSWORD=your-mail-password

# Admin email (to receive error reports)
ADMIN_MAIL=your-admin-email

# Google Maps API Key (for displaying property locations)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key